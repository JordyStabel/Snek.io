<configuration>
    <appender class="ch.qos.logback.core.ConsoleAppender" string="STDOUT">
        <encoder class="ch.qos.logback.classic.encoder.PatternLayoutEncoder">
            <pattern>%date [%-16thread{16}] [%-5level] %-48logger{48} - %message%n</pattern>
        </encoder>
    </appender>

    <logger level="DEBUG" string="com.s3.SnekIO"/>
    <logger level="INFO" string="org.eclipse.jetty"/>

    <root level="ERROR">
        <appender-ref ref="STDOUT"/>
    </root>

</configuration>